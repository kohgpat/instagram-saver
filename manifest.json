{
  "name": "Instagram Saver",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Save shots from instagram",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "background",
    "https://www.instagram.com/"
  ]
}